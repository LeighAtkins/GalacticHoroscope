{
  "name": "galactichoroscope",
  "version": "1.0.0",
  "description": "A fun, interactive application that provides users with their astrological star sign and a personalized, AI-generated fortune.",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "serve": "node dist/src/server.js",
    "dev:api": "ts-node src/server.ts",
    "watch:api": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/server.ts",
    "simple-server": "node src/simple-server.js",
    "watch:simple": "nodemon --watch 'src/**/*.js' --exec 'node' src/simple-server.js",
    "web": "node server.js",
    "watch:web": "nodemon server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LeighAtkins/GalacticHoroscope.git"
  },
  "keywords": [
    "astrology",
    "horoscope",
    "zodiac",
    "fortune",
    "node",
    "cli"
  ],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/LeighAtkins/GalacticHoroscope/issues"
  },
  "homepage": "https://github.com/LeighAtkins/GalacticHoroscope#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "gsap": "^3.12.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "three": "^0.158.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.15.17",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^8.32.0",
    "@typescript-eslint/parser": "^8.32.0",
    "eslint": "^9.26.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}